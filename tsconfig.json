{
  "compilerOptions": {
      "module": "commonjs",
      "esModuleInterop": true,
      "target": "esNext",
      "noImplicitAny": true,
      "moduleResolution": "node",
      "sourceMap": true,
      "outDir": "dist",
      "baseUrl": ".",
      "paths": {
          "*": [
              "node_modules/*",
              "src/types/*"
          ]
      },
    "noUnusedLocals": false
  },
  "include": [
      "src/**/*"
  ]
}
